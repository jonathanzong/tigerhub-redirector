{
  "name": "Tigerhub Redirector",
  "version": "0.1",
  "description": "For Princeton students. Takes you to Tigerhub -- after you've logged in but before Tigerhub has realized it.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": { 
    "page":"bg.html"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
    "matches": ["https://puaccess.princeton.edu/pusa/psftError.jsp?errorMsg1=Your+User+ID+and%2For+Password+are+invalid.&errorMsg2="],
    "js": ["content.js"]
    }
  ],
  "permissions": ["tabs"]
}
